<div class="col-sm-12">
  <p id="notice"><%= notice %></p>

  <h1>Pins</h1>
  <table id="pins" class="table table-condensed">
    <thead>
    <tr>
      <th>Name</th>
      <th>Image</th>
      <th>Board</th>
      <th colspan="3"></th>
    </tr>
    </thead>
    <tbody class="page">
    <%= render @pins %>
    </tbody>
  </table>
  <%= link_to 'New Pin', new_pin_path %>
  <%= paginate @pins %>
</div>
